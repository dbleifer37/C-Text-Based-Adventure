#include <stdio.h>
typedef struct monsters monsters;
struct monsters
{
    int mhealth;
    int mattack;
    int mskill;
    char mname[20];
};

int main()
{
    monsters mewtwo = {8,6,7,"Mewtwo"};
    printf("Name= %s, Health= %d, Mind Control= %d, Skill= %d",mewtwo.mname, mewtwo.mhealth,mewtwo.mattack,mewtwo.mskill);
    return 0;
}




#include <stdio.h>
typedef struct monsters monsters;
struct monsters
{
    int mhealth;
    int mattack;
    int mskill;
    char mname[20];
};

int main()
{
    monsters squirtle = {5,3,4,"Squirtle"};
    printf("Name= %s, Health= %d, Watergun= %d, Skill= %d",squirtle.mname, squirtle.mhealth,squirtle.mattack,squirtle.mskill);
    return 0;
}
