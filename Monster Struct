#include <stdio.h>
typedef struct monsters monsters;
struct monsters
{
    int mHP;
    int mATK;
    int mskill;
    char mname[25];
};

int main()
{
monsters elf = {10,5,5,"Elf"};
printf("%d %d %d %s",elf.mHP,elf.mATK,elf.mskill,elf.mname);
return 0;
}

#include <stdio.h>
typedef struct monsters monsters;
struct monsters
{
    int mhealth;
    int mattack;
    int mskill;
    char mname[20];
};

int main()
{
    monsters jynx = {8,6,7,"Jynx"};
    printf("Name= %s, Health= %d, Humiliate= %d, Skill= %d",jynx.mname, jynx.mhealth,jynx.mattack,jynx.mskill);
    return 0;
}




#include <stdio.h>
typedef struct monsters monsters;
struct monsters
{
    int mhealth;
    int mattack;
    int mskill;
    char mname[20];
};

int main()
{
    monsters pikachu = {5,2,3,"Pikachu"};
    printf("Name= %s, Health= %d, Thunderbolt= %d, Skill= %d",pikachu.mname, pikachu.mhealth,pikachu.mattack,pikachu.mskill);
    return 0;
}




